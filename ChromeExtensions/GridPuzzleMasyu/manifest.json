{
  "manifest_version": 3,
  "name": "Masyu Solver",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://gridpuzzle.com/*", "http://gridpuzzle.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["masyu_solver.py", "inject.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
